    <!--Styles-->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/noty/3.1.4/noty.min.css">
    <link rel="stylesheet" href="/css/layout.css">
    <%- style %>
    <title>
        <%= title%>
    </title>
    <link rel="icon" type="image/x-icon" href="/images/csv-logo.png">
</head>

<body>
    <!-- <header>
        <h2><img src="/images/csv-logo.png" alt="" width="50px">Upload</h2>
    </header> -->
    <nav class="navbar bg-body-tertiary">
        <div class="container-fluid">
          <a class="navbar-brand" href="#">
            <img src="/images/csv-logo.png" alt="Logo" width="30" height="24" class="d-inline-block align-text-top">
            CSV Upload
          </a>
        </div>
    </nav>

    <div class="container-fluid py-3">
        <%- body %>
    </div>
